generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id      Int       @id @default(autoincrement())
  name    String    @unique
  price   Int
  type    String
  user    User?     @relation(fields: [userId], references: [id])
  userId  Int
  deleted DateTime?
}

model User {
  id       Int       @id @default(autoincrement())
  username String
  email    String    @unique
  password String
  order    Order[]
  deleted  DateTime?
}
